{
    "env": {
      "browser": true,
      "es6": true,
      "node": true,
      "jasmine": true
    },
    "extends": ["google", "plugin:prettier/recommended"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module",
      "allowImportExportEverywhere": true
    },
    "parser": "babel-eslint",
    "plugins": ["react"],
    "rules": {
      "react/jsx-uses-react": 1,
      "require-jsdoc": 0,
      "react/jsx-uses-vars": 1,
      "quotes": ["error", "single"],
      "max-len": ["error", { "code": 80 }],
      "no-invalid-this": 0
    }
  }